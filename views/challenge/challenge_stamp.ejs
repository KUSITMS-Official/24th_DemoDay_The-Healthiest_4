    <!-- Modal -->
    <div
      class="modal fade"
      id="success"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content">
          <div class="modal-body">
            <br />
            <br />
            <div class="modal-h">챌린지 인증에 성공하였습니다.</div>
            <br />
            <br />
            <button
              class="modal-b"
              type="button"
              data-dismiss="modal"
              aria-label="Close"
            >
              스탬프 페이지로 돌아가기
            </button>
            <br />
            <br />
          </div>
        </div>
      </div>
    </div>
    <div
      class="modal fade"
      id="fail"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content">
          <div class="modal-body">
            <br />
            <br />
            <div class="modal-h">챌린지 인증에 실패하였습니다.</div>
            <br />
            <br />
            <div>
              <button
                class="modal-btn-reason"
                data-toggle="modal"
                data-target="#fail-reason"
                type="button"
                data-dismiss="modal"
                aria-label="Close"
                onclick="changeContent(<%=certification%>)"
              >
                인증 실패 이유 확인하기
              </button>
            </div>
            <div>
              <button
                class="modal-btn"
                type="button"
                data-dismiss="modal"
                aria-label="Close"
              >
                스탬프 페이지로 돌아가기
              </button>
            </div>
            <br />
            <br />
          </div>
        </div>
      </div>
    </div>
    <div
      class="modal fade"
      id="fail-reason"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
